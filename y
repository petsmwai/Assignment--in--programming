#include<stdio.h>
int main(){
    int height ,weight,phone;
    printf("enter height");
    scanf("%d",&height);
    printf("enter weight");
    scanf("%d",&weight);
    printf("phone");
    scanf("%d",&phone);
    printf("height ;%d",height);
    printf("weight ;%d",weight);
    printf ("phone ;%d",phone);
    return 0;
    }




